{
  "dashboard": {
    "title": "缓存监控",
    "tags": ["cache", "llm", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "缓存命中率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(llm_cache_hit_total[5m]) / (rate(llm_cache_hit_total[5m]) + rate(llm_cache_miss_total[5m])) * 100",
            "legendFormat": "{{business}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "命中率", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "缓存未命中率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(llm_cache_miss_total[5m]) / (rate(llm_cache_hit_total[5m]) + rate(llm_cache_miss_total[5m])) * 100",
            "legendFormat": "{{business}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "未命中率"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "缓存错误率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(llm_cache_error_total[5m])",
            "legendFormat": "{{business}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "错误次数/秒"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "缓存操作统计",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(llm_cache_hit_total[5m])",
            "legendFormat": "命中 - {{business}}",
            "refId": "A"
          },
          {
            "expr": "rate(llm_cache_miss_total[5m])",
            "legendFormat": "未命中 - {{business}}",
            "refId": "B"
          },
          {
            "expr": "rate(llm_cache_error_total[5m])",
            "legendFormat": "错误 - {{business}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "操作次数/秒"},
          {"format": "short"}
        ]
      }
    ]
  }
}

