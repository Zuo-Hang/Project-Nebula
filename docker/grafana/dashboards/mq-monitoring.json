{
  "dashboard": {
    "title": "MQ 监控",
    "tags": ["mq", "rocketmq", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "MQ 发送速率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(ddmq_producer_total[1m])",
            "legendFormat": "{{topic}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "发送次数/秒"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "MQ 消费速率",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(ddmq_req_total[1m])",
            "legendFormat": "{{topic}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "消费次数/秒"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "MQ 消费延迟 (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(HandlerMsg_duration_ms_bucket[5m]))",
            "legendFormat": "P95 延迟",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "延迟"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "MQ 消费重试次数",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(ddmq_req_retry_total[5m])",
            "legendFormat": "重试次数 - retry={{retry}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "重试次数/秒"},
          {"format": "short"}
        ]
      }
    ]
  }
}

